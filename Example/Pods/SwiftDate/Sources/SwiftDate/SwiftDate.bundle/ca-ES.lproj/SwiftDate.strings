"colloquial_p_dd"					=	"fa %d dies";		// day,past,plural:			"3 days ago"

"colloquial_f_h"					=	"en una hora";		// hour,future,singular:	"in one hour"
"colloquial_f_hh"					=	"en %ld hores";		// hour,future,plural:		"in 3 hours"
"colloquial_p_h"					=	"fa una hora";		// hour,past,singular:		"one hour ago"
"colloquial_p_hh"					=	"fa %d hores";		// hour,past,plural:		"3 hours ago"

"colloquial_f_M"					=	"en un minut";      // minute,future,singular:	"in one minute"
