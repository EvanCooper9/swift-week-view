"colloquial_p_y"					=	"passat any";		// year,past,singular:      "last year"
"colloquial_p_yy"					=	"%d";				// year,past,plural:        "2015"

"colloquial_f_m"					=	"pròxim mes";		// month,future,singular:	"next month"
